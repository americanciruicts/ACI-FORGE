{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api-forge.americancircuits.net/api/:path*"
    },
    {
      "source": "/health",
      "destination": "https://api-forge.americancircuits.net/health"
    }
  ]
}
